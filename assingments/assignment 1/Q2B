#include <stdio.h>
// Function to compute the nth Fibonacci number iteratively
int fib(int n) {
    if (n < 0) {
        // Handle invalid input
        return -1;
    }
    if (n == 0) return 0;
    if (n == 1) return 1;

    int prev = 0, curr = 1, next;
    for (int i = 2; i <= n; i++) {
        next = prev + curr;
        prev = curr;
        curr = next;
    }
    return curr;
}

// Example usage
int main() {
    int n = 10;
    printf("Fibonacci number at position %d is %d\n", n, fib(n));
    return 0;
}