#a0 = dst base address, a1 = src base address, s0 = i

strpy:
    addi sp, sp -4
    sw s0, 0(sp) #saves on stack
    addi s0, zero, 0 #i = 0
    
loop:
    add t1, a1, s0 #t1 = src[i] address
    add t2 a0, s0 #t2 = dst[i] address
    sb t3, 0(t1) #t3 = src[i]
    sb t3, 0(t2) #dst[i] = src[i]
    beq t3, zero, done #if src[i] = 0, done
    addi s0, s0, 1 #i++
    
done:
    lw s0, 0(sp)
    addi sp, sp, 4
    jr ra


    